            <version>6.0.7.Final</version>
        </dependency>
        <!-- Lombok -->
        <dependency> 
            <groupId>org.projectlombok</groupId> 
            <artifactId>lombok</artifactId> 
            <version>1.16.16</version> 
            <scope>provided</scope> 
        </dependency>
        <!--CalendÃ¡rio-->
        <dependency>
            <groupId>com.toedter</groupId>
